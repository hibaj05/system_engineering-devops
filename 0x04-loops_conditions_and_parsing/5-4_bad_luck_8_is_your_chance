#!/usr/bin/env bash
# This script displays different messages based on loop iteration using if-elif-else.

count=1 # Initialize count to 1 for the 1st iteration
while [ "$count" -le 10 ]; do # Loop while count is less than or equal to 10
    if [ "$count" -eq 4 ]; then
        echo "bad luck"
    elif [ "$count" -eq 8 ]; then
        echo "good luck"
    else
        echo "Best School"
    fi
    count=$((count + 1)) # Increment count for the next iteration
done

