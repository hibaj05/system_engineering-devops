#!/usr/bin/env bash
# This script displays numbers from 1 to 20, with special messages for 4, 9, and 17.

count=1
while [ "" -le 20 ]; do
    echo ""
    case "" in
        4)
            echo "bad luck from China"
            ;;
        9)
            echo "bad luck from Japan"
            ;;
        17)
            echo "bad luck from Italy"
            ;;
        *)
            ;;
    esac
    count=1
done
