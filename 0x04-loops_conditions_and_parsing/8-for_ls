#!/usr/bin/env bash
# This script displays the content of the current directory,
# listing only the part of the name after the first dash (-).
# It uses a for loop and does not display hidden files.

for file in *; do
    if [ -f "$file" ]; then
        if [[ "$file" == *-* ]]; then
            echo "${file#*-}"
        fi
    fi
done

