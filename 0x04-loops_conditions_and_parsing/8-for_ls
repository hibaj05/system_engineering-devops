#!/usr/bin/env bash
# This script lists all non-hidden files in the current directory, sorted, and displays only the part after '-' if present

find . -maxdepth 1 -type f ! -name ".*" -printf "%f\n" | sort | while IFS= read -r file; do
  if [[ "$file" == *-* ]]; then
    echo "${file#*-}"
  else
    echo "$file"
  fi
done

